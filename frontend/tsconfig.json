{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "moduleResolution": "nodenext",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "strict": true,
    "target": "esnext",
    "module": "NodeNext",
    "lib": [
      "esnext",
      "dom"
    ],
    "types": [
      "react",
      "react-native"
    ]
  }
}